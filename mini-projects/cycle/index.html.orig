<div id="container"></div>
<script src="scripts/raphael-min.js"></script>
<script src="scripts/paths.js"></script>
<script>
    const runIn = ['GB', 'CA', 'DE', 'NL', 'ES', 'FR'];
    const R = Raphael("container", 1000, 900);
    const notRun = {
	"fill": "#d3d3d3",
	"stroke": "#fff",
	"stroke-opacity": "1",
	"stroke-linejoin": "round",
	"stroke-miterlimit": "4",
	"stroke-width": "0.75",
	"stroke-dasharray": "none"
    };

    const run = {
	"fill": "#8e44ad",
	"stroke": "#fff",
	"stroke-opacity": "1",
	"stroke-linejoin": "round",
	"stroke-miterlimit": "4",
	"stroke-width": "0.75",
	"stroke-dasharray": "none"
    };

    for (var state in usMap) {
	if (runIn.indexOf(state) >= 0) {
	    R.path(usMap[state]['path']).attr(run);
	} else {
	    R.path(usMap[state]['path']).attr(notRun);
	}
    }
</script>
